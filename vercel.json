{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/mvp/**"]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://aca-services-openswetro.grayhill-df08ba08.eastus2.azurecontainerapps.io/api/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/dist/mvp/browser/assets/$1"
    },
    {
      "source": "/(.*\\.js|.*\\.css|.*\\.ico|.*\\.png|.*\\.jpg|.*\\.svg|.*\\.woff2|.*\\.woff|.*\\.ttf|.*\\.json)",
      "destination": "/dist/mvp/browser/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}
