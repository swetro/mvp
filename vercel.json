{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://aca-services-openswetro.grayhill-df08ba08.eastus2.azurecontainerapps.io/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/mvp/browser/assets/$1"
    },
    {
      "src": "/(.*\\.js|.*\\.css|.*\\.ico|.*\\.png|.*\\.jpg|.*\\.svg|.*\\.woff2|.*\\.woff|.*\\.ttf|.*\\.json)",
      "dest": "/dist/mvp/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}

